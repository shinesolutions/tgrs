{
  "name": "server",
  "private": true,
  "version": "0.0.0",
  "engines": {
    "node": ">=20.0.0 <21.0.0",
    "npm": "please-use-yarn",
    "yarn": ">= 1.22.17"
  },
  "scripts": {
    "codegen": "graphql-codegen --config codegen.yml",
    "start": "./scripts/start.sh",
